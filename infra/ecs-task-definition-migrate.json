{
  "family": "andas-migrate",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::095907292083:role/andas-ecs-execution-role",
  "taskRoleArn": "arn:aws:iam::095907292083:role/andas-ecs-task-role",
  "containerDefinitions": [
    {
      "name": "migrate",
      "image": "PLACEHOLDER_IMAGE",
      "essential": true,
      "secrets": [
        {
          "name": "DATABASE_URL",
          "valueFrom": "arn:aws:secretsmanager:ap-southeast-5:095907292083:secret:andas/prod/database-url-2b07oM"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/andas/migrate",
          "awslogs-region": "ap-southeast-5",
          "awslogs-stream-prefix": "migrate",
          "awslogs-create-group": "true"
        }
      }
    }
  ]
}
